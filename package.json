{
  "name": "ajp-app",
  "version": "1.0.0",
  "main": "app.js",
  "dependencies": {
    "bootstrap": "4.0.0",
    "css-loader": "^0.28.11",
    "final-form": "^4.5.2",
    "node-sass": "^4.8.3",
    "npm-run-all": "^4.1.2",
    "react": "^15.6.2",
    "react-dom": "15.6.1",
    "react-final-form": "^3.3.1",
    "react-modal": "2.2.2",
    "react-router-dom": "4.2.2",
    "react-scripts": "1.1.4",
    "reactstrap": "^5.0.0-beta.3",
    "sass-loader": "^6.0.7",
    "style-loader": "0.18.2",
    "styled-components": "^3.2.6",
    "validator": "8.0.0",
    "yarn": "^1.2.1"
  },
  "devDependencies": {
    "gulp-autoprefixer": "^5.0.0"
  },
  "scripts": {
    "build-scss": "node-sass --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-scss": "npm run build-scss && node-sass --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-scss start-js",
    "build": "npm run build-scss && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "author": "Paulina Lapinska",
  "license": "ISC",
  "description": "AJP Sentiment Analysis",
  "repository": "https://gitlab.com/ajp-gorzow/paulina-lapinska.git"
}
